{
  "presets": [ ["env", { "targets": { "node": "current" } }] ],

  "env": {
    "browsers": {
      "presets": [ ["env", { "targets": { "browsers": ["last 2 versions"] } }] ],
      "plugins": ["add-module-exports"]
    },

    "node4": {
      "presets": [ ["env", { "targets": { "node": 4.0 } }] ],
      "plugins": ["add-module-exports"]
    },

    "node6": {
      "presets": [ ["env", { "targets": { "node": 6.0 } }] ],
      "plugins": ["add-module-exports"]
    },

    "node7": {
      "presets": [ ["env", { "targets": { "node": 7.0 } }] ],
      "plugins": ["add-module-exports"]
    }
  }
}
